<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">

        <link href="../public/styles.css" rel="stylesheet" type="text/css">

        <title>View Patient Record</title>

    </head>
    <body>
      <% include ./resources/navHeader.ejs %>
        <form action="http://localhost:3003/view_appointment_treatment_record" method="post">

          <% unclearedAppointments.forEach(function (ans) { %>
              <% var record = {SSN: ans.PatientSSN, date: ans.date}; %>
              <input type="radio"  name="records" id="records" value= <%=JSON.stringify(record)%>> <%=ans.firstname + " " + ans.lastname + " " + ans.date%></br>
          <% }); %>

            <button type="submit">Submit</button>
        </form>
        <br/>
    <script>

    </script>

    </body>
</html>
